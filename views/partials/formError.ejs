<% if (locals.errors && locals.errors.length > 0) { %>
<div
  class="mt-3 mb-3 p-1 bg-red-100 border border-red-400 text-red-700 rounded"
>
  <ul class="list-disc flex flex-col gap-3 mt-3">
    <% errors.forEach(error => { %>
    <li class=""><%= error.msg %></li>
    <% }) %>
  </ul>
</div>
<% } %>
